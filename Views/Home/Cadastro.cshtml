@using newsletter.Models
@model List<Usuario>
<h2>Usuário cadastrado com Sucesso!</h2>

<table>
    <thead>
        <tr>
            <th>Nome</th>
            <th>Email</th>
            <th>Gênero</th>
        </tr>
    <tbody>
        @foreach (Usuario u in Model)
            {
            <tr>
                <td>@u.Nome</td>
                <td>@u.Email</td>
                <td>@u.Genero</td>
            </tr>
            }
    </tbody>
    </thead>    
</table>

<a href="/">Voltar</a>